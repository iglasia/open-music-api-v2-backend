{
  "env": {
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "no-console": "off",
    "linebreak-style": ["error", "windows"],
    "quotes": "off",
    "import/newline-after-import": "off",
    "eol-last": "off"
  }
}
